[general]
status_path = "~/.vdirsyncer/status/"

[pair personal_calendar]
a = "personal_calendar_local"
b = "personal_calendar_remote"
collections = null
metadata = ["displayname", "color"]

[storage personal_calendar_local]
type = "filesystem"
path = "~/.calendars/personal/"
fileext = ".ics"

[storage personal_calendar_remote]
type = "caldav"
url.fetch = ["command", "pass", "webdav/caldav/personal/url"]
username.fetch = ["command", "pass", "webdav/username"]
password.fetch = ["command", "pass", "webdav/password"]



[pair personal_contacts]
a = "personal_contacts_local"
b = "personal_contacts_remote"
collections = null

[storage personal_contacts_local]
type = "filesystem"
path = "~/.contacts/personal/"
fileext = ".vcf"

[storage personal_contacts_remote]
type = "carddav"
url.fetch = ["command", "pass", "webdav/carddav/personal/url"]
username.fetch = ["command", "pass", "webdav/username"]
password.fetch = ["command", "pass", "webdav/password"]



